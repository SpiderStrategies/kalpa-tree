{
  "name": "scoreboard-tree",
  "version": "0.0.2",
  "description": "A Scoreboard tree rendered from a stream using d3",
  "main": "index.js",
  "dependencies": {
    "d3": "^3.5.2",
    "lesshat": "^3.0.2"
  },
  "devDependencies": {
    "lodash": "^2.4.1",
    "browserify": "^3.46.0",
    "JSONStream": "^0.8.3",
    "request": "^2.36.0",
    "less": "^1.7.0",
    "watchify": "^0.10.2"
  },
  "scripts": {
    "bundle": "./node_modules/.bin/browserify -r ./index.js:tree -r http -r JSONStream > example/bundle.js -d",
    "watch": "./node_modules/.bin/watchify -r ./index.js:tree -r http -r JSONStream -o example/bundle.js -d",
    "start": "node example/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SpiderStrategies/scoreboard-tree.git"
  },
